<script>
$(document).ready(function(){
	
	$('.qty-item').click(function(){
            $('.qty-item').removeClass('active');
            $(this).addClass('active');		
	});
	$('input:radio').change(function(e){$(this).siblings('input:radio').prop('checked',$(this).prop('checked'));});
        $('.border-style a').click(function(){$('a.size-next').attr('href',this.href);});
//        window.onhashchange = function(){
//           
//        };
        $(".size-next").click(function(){
            if(!$('.qty-item').hasClass('active')){
                $('.alert').addClass('active')
                setTimeout(function(){$('.alert').removeClass('active');}, 1500);

            }else{
                if (document.getElementById(location.hash.substr(1))){
                    $('.content.select-size').hide();               
                    $(location.hash).addClass('active').show();
                }else{
                    $('.content.select-size').show();
                    $('.row.select').hide();
                }
            }
        });
        $(document).on('click', '.row.select.active button.btn', function() {
            if(!$('.row.select.active .qty-item').hasClass('active')){
                $('.alert').addClass('active')
                setTimeout(function(){$('.alert').removeClass('active');}, 1500);

            }else{$(this).submit();}
        })
        $(document).on("click",".row.select.active .back-btns",function() {
            location.reload();
            removeHash();
        });
        
});
</script>
</body>
